{
  "name": "neos/utility-unicode",
  "description": "Flow Unicode Utilities",
  "type": "library",
  "homepage": "http://flow.neos.io",
  "license": "MIT",
  "config": {
    "bin-dir": "bin"
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6.1",
    "phpunit/phpunit": "~9.1"
  },
  "replace": {
    "symfony/polyfill-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Neos\\Utility\\Unicode\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neos\\Flow\\Utility\\Unicode\\Tests\\": "Tests"
    }
  }
}
