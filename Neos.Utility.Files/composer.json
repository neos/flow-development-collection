{
  "name": "neos/utility-files",
  "description": "Flow Files Utilities",
  "type": "library",
  "homepage": "http://flow.neos.io",
  "license": "MIT",
  "require": {
    "php": "^7.3 || ^8.0",
    "neos/error-messages": "*"
  },
  "require-dev": {
    "mikey179/vfsstream": "~1.6",
    "phpunit/phpunit": "~9.0"
  },
  "autoload": {
